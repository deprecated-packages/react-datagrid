{
  "parser": "babel-eslint",
  "rules": {
    "flowtype/boolean-style": [
      2,
      "boolean"
    ],
    "flowtype/define-flow-type": 1,
    "flowtype/delimiter-dangle": [
      2,
      "never"
    ],
    "flowtype/generic-spacing": [
      2,
      "never"
    ],
    "flowtype/no-weak-types": 0,
    "flowtype/require-parameter-type": 0,
    "flowtype/require-return-type": [
      0,
      "always",
      {
        "annotateUndefined": "never"
      }
    ],
    "flowtype/require-valid-file-annotation": 2,
    "flowtype/semi": [
      2,
      "always"
    ],
    "flowtype/space-after-type-colon": [
      2,
      "always"
    ],
    "flowtype/space-before-generic-bracket": [
      2,
      "never"
    ],
    "flowtype/space-before-type-colon": [
      2,
      "never"
    ],
    "flowtype/type-id-match": [
      2,
      "^([A-Z][a-z0-9]+)+Type$"
    ],
    "flowtype/union-intersection-spacing": [
      2,
      "always"
    ],
    "flowtype/use-flow-type": 1,
    "flowtype/valid-syntax": 1,
    "react/jsx-boolean-value": [
      2,
      "always"
    ],
    "react/jsx-equals-spacing": 2,
    "react/jsx-indent-props": [
      2,
      "tab"
    ],
    "react/jsx-indent": [
      0,
      "tab"
    ],
    "react/jsx-key": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-quotes": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-is-mounted": 2,
    "react/no-multi-comp": 0,
    "react/no-string-refs": 0,
    "react/no-unknown-property": 2,
    "react/prefer-es6-class": 2,
    "react/prop-types": 2,
    "react/react-in-jsx-scope": 2,
    "no-dupe-args": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-var": 2,
    "no-empty-character-class": 2,
    "no-extra-semi": 2,
    "no-extra-parens": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "no-obj-calls": 2,
    "no-unreachable": 2,
    "no-unexpected-multiline": 2,
    "no-sparse-arrays": 2,
    "no-negated-in-lhs": 2,
    "no-irregular-whitespace": 2,
    "no-invalid-regexp": 2,
    "no-cond-assign": 2,
    "block-scoped-var": 2,
    "max-nested-callbacks": [
      2,
      3
    ],
    "max-params": [
      2,
      5
    ],
    "max-statements": [
      2,
      20
    ],
    "max-depth": [
      2,
      5
    ],
    "comma-dangle": [
      0,
      "always-multiline"
    ],
    "no-dupe-keys": 2,
    "no-empty": 2,
    "quotes": [
      0,
      "double"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "init-declarations": [
      2,
      "always"
    ],
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-use-before-define": 2,
    "callback-return": 0,
    "consistent-return": 2,
    "default-case": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-catch-shadow": 2,
    "no-implicit-coercion": 2,
    "guard-for-in": 2,
    "global-require": 2,
    "handle-callback-err": 2,
    "dot-notation": 2,
    "no-unused-vars": 2,
    "no-alert": 2,
    "no-with": 2,
    "no-console": 0,
    "no-debugger": 2,
    "no-shadow-restricted-names": 0,
    "no-multi-str": 0,
    "curly": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "no-undefined": 2,
    "no-undef-init": 2,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "namespace": false,
    "it": false,
    "xit": false,
    "xdescribe": false,
    "before": false,
    "after": false,
    "afterEach": false,
    "beforeEach": false,
    "describe": false,
    "latte": false,
    "constant": false,
    "$": false,
    "tlap": false,
    "jsPlumb": false,
    "ENVIRONMENT_PROFILE": false
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true,
    "arrowFunctions": true,
    "destructuring": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "forOf": true,
    "generators": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": true
  },
  "plugins": [
    "react",
    "flowtype"
  ]
}